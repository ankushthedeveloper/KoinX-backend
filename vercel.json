{
    "version":2,
    "builds":[{"src":"api/server.js","use":"@vercel/node"}],
   "rewrites": [
        { "source": "/", "destination": "api/server.js" },
        { "source": "/api/(.*)", "destination": "api/server.js" }
    ],

    "functions": {
   "api/server.js": {
      "maxDuration": 60
    }
  }

}
