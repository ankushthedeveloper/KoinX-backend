{
    "version":2,
    "builds":[{"src":"./server.js","use":"@vercel/node"}],
   "rewrites": [
        { "source": "/", "destination": "/server.js" },
        { "source": "/api/(.*)", "destination": "/server.js" }
    ],

    "functions": {
    "routes/stats.js": {
      "maxDuration": 60
    },
    "routes/deviation.js": {
      "maxDuration": 60
    },
    "*.js": {
      "maxDuration": 60 
    }
  }

}
